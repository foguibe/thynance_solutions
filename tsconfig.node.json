{
    "compilerOptions": {
      "target": "es2017",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "jsx": "react-jsx",
      "sourceMap": true,
      "outDir": "dist" ,
      "declaration": false,
      "noEmit": false
    },
    "include": ["seed.ts"],
    "exclude": ["node_modules"]
  }